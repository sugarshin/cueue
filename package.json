{
  "name": "cueue",
  "version": "0.0.1",
  "description": "Tiny, Easy queueing on Promise based.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "repository": "https://github.com/sugarshin/cueue",
  "author": "sugarshin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sugarshin/cueue/issues"
  },
  "homepage": "https://github.com/sugarshin/cueue#readme",
  "keywords": [
    "queue",
    "async",
    "queueing",
    "promise"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --progress",
    "test": "BABEL_ENV=test ava",
    "test:coverage": "nyc npm test",
    "test:coverage:report": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src test.js",
    "build:commonjs": "BABEL_ENV=commonjs babel -d lib src",
    "build:es": "BABEL_ENV=es babel -d es src",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run lint && npm test && npm run build",
    "favicon": "DIR=demo && curl https://sugarshin.net/favicon.ico > $DIR/favicon.ico"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "coveralls": "^2.12.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "html-webpack-include-assets-plugin": "^0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "nyc": "^10.1.2",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
